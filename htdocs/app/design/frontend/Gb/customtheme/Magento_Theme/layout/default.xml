<?xml version="1.0" encoding="UTF-8"?>
	

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="logo">
            <arguments>
                <argument name="logo_file" xsi:type="string">images/logo.svg</argument>
                <argument name="logo_width" xsi:type="number">188</argument>
                <argument name="logo_height" xsi:type="number">52</argument>
                <argument name="logo_alt" xsi:type="string">GB Dentaire</argument>
            </arguments>
        </referenceBlock>
		
	<!--	<referenceContainer name="header.panel">
            <block class="Magento\Cms\Block\Block" name="block_identifier">
                <arguments>
                    <argument name="block_id" xsi:type="string">my_cms_block_identifier</argument>
                </arguments>
            </block>
        </referenceContainer>-->
	
		
		<!--<referenceBlock name="privacy-policy-link" remove="true"/>-->
		
		
		
		<referenceBlock name="footer_links">
        <block class="Magento\Framework\View\Element\Html\Link\Current" name="qui-sommes-nous-link">
            <arguments>
                <argument name="label" xsi:type="string">Qui sommes nous</argument>
                <argument name="path" xsi:type="string">qui-sommes-nous</argument>
            </arguments>
        </block>
			
			<block class="Magento\Framework\View\Element\Html\Link\Current" name="service-clients-link">
            <arguments>
                <argument name="label" xsi:type="string">Service clients</argument>
                <argument name="path" xsi:type="string">service-clients</argument>
            </arguments>
        </block>
			
			<block class="Magento\Framework\View\Element\Html\Link\Current" name="conditions-generales-de-vente-link">
            <arguments>
                <argument name="label" xsi:type="string">Conditions générales de vente</argument>
                <argument name="path" xsi:type="string">conditions-generales-de-vente</argument>
            </arguments>
        </block>
			
			
    </referenceBlock>
		
		<referenceBlock name="catalog-search-advanced-link" remove="true"/>
		<referenceBlock name="search-term-popular-link" remove="true"/>
		<referenceBlock name="sales-guest-form-link" remove="true"/>
		
		
			



		
		
    </body>
</page>


 
